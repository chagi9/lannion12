<h1><%= (t :liste_upload) %></h1>

<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @uploads.each do |upload| %>
  <tr>
    <td><%= link_to (t :voir), upload %></td>
    <td><%= link_to (t :editer), edit_upload_path(upload) %></td>
    <td><%= link_to (t :detruire), upload, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to (t :nouveau_upload), new_upload_path %>
