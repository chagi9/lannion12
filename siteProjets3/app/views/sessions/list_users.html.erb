<%= title(t :manage_admin) %>
<h1><%= (t :manage_admin) %></h1>
<ul>
  <% for user in @users %>
      <li><%= h(user.name) %>
        <%= link_to image_tag("detruire.png", :size => '20x20', :title => (t :delete)) ,user, :method => :delete, :confirm => (t :confirm_sure),
                    :title => "Delete #{user.name}" %>
      </li>
  <% end %>
</ul>

<%= link_to (t :add), :controller => 'sessions', :action => 'add_user' %>   <%= link_to (t :back_admin), :controller => 'sessions', :action => 'index' %>
